#include <stdio.h>
int main(){
    
    int H,M,S;
    puts("Digite as horas:");
    scanf("%d",&H);
    puts("Digite os minutos:");
    scanf("%d",&M);
    puts("Digite os segundos");
    scanf("%d",&S);
    printf("O tempo total convertido em segundos é %d", H*3600 + M*60 + S);
    
    return 0;
}
